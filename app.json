{
  "expo": {
    "name": "bayzati",
    "slug": "bayzati",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bayzati-logo-icon.png",
    "scheme": "bayzatiexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/bayzati-logo-icon.png",
        "backgroundImage": "./assets/images/bayzati-logo-icon.png",
        "monochromeImage": "./assets/images/bayzati-logo-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "usesCleartextTraffic": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/bayzati-logo-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/b-logo-white.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0B3D2E",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
